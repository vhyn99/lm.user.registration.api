# src/main/resources/graphql/schema.graphqls
type Query {
  _serviceHealthCheck: String
}


type Mutation {
    registerUser(userInput: User!): String
}

input User {
    username: String!
    email: String!
    password: String
    firstName: String
    lastName: String
    phone: String
    address: String
    city: String
    state: String
    country: String
    zip: String
    accountName: String
}

